<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>mini_project_shared</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginManagement1</servlet-name>
    <servlet-class>kr.co.yagaja.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginManagement1</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginManagement2</servlet-name>
    <servlet-class>kr.co.yagaja.controller.JoinController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginManagement2</servlet-name>
    <url-pattern>/join</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginManagement3</servlet-name>
    <servlet-class>kr.co.yagaja.controller.LogoutController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginManagement3</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginManagement4</servlet-name>
    <servlet-class>kr.co.yagaja.controller.IdCheckController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginManagement4</servlet-name>
    <url-pattern>/check</url-pattern>
  </servlet-mapping>
</web-app>